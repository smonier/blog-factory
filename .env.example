# Jahia Development Environment Configuration
# Copy this file to .env and fill in your actual values

# Jahia Server Configuration
JAHIA_SERVER_URL=http://localhost:8080
JAHIA_USERNAME=root
JAHIA_PASSWORD=root1234

# Blog Service Configuration
# CSRF Token for blog-service API calls
BLOG_SERVICE_CSRF_TOKEN=your-csrf-token-here

# Blog Service API Base URL
BLOG_SERVICE_API_URL=/modules/graphql
